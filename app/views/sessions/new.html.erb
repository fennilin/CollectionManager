<%= form_for :session, :url => login_path, :method => :post do |f| %>

<h1>Log In! </h1>
<br>

  <%= f.label "Email address" %>
  <%= f.text_field :email %>
  <br><br>
  <%= f.label "Password" %>
  <%= f.password_field :password %>

  <div class="actions">
    <br/>
    <%= f.submit "Log in" %>
  </div>

<% end %>
